{
    "name": "luppakorva/laravel-mentions",
    "description": "Update to jameslkingsley/laravel-mentions for Laravel 6.",
    "keywords": [
        "laravel",
        "mentions"
    ],
    "homepage": "https://github.com/luppakorva/laravel-mentions",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike",
            "email": "",
            "homepage": "https://github.com/luppakorva",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^5.5",
        "illuminate/database": "^5.5",
        "illuminate/http": "^5.5",
        "illuminate/routing": "^5.8"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "6.3",
        "orchestra/testbench": "3.5"
    },
    "autoload": {
        "psr-4": {
            "Kingsley\\Mentions\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kingsley\\Mentions\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor\\bin\\phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kingsley\\Mentions\\MentionServiceProvider"
            ]
        }
    }
}
